{
    "name": "code-template-replace",
    "displayName": "code Template Replace",
    "description": "replace your own code snippet",
    "version": "0.2.0",
    "publisher": "heekei",
    "engines": {
        "vscode": "^1.5.0"
    },
    "icon": "images/icon.ico",
    "bugs": {
        "url": "https://github.com/heekei/code-template-replace/issues",
        "email": "heekei@foxmail.com"
    },
    "homepage": "http://www.heekei.cn",
    "repository": {
        "type": "git",
        "url": "https://github.com/heekei/code-template-replace.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.codeReplace"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.codeReplace",
                "title": "Code Replace"
            }
        ],
        "keybindings": [
            {
                "key": "ctrl+alt+0",
                "command": "extension.codeReplace"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32",
        "fs": "4.4.0"
    }
}